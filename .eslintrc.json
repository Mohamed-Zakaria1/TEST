{
  "env": {
      "browser": true,
      "es2021": true,
      "node": true
  },
  "extends": [
      "eslint:recommended",
      "google",
      "plugin:react/recommended",
      "plugin:tailwindcss/recommended"
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "react"
  ],
  "rules": {
      "linebreak-style": 0,
      "indent": ["error", 4],
      "max-len": ["error", 120]
  }
}
